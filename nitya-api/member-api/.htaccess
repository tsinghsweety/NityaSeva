# Turn rewrite engine on
Options +FollowSymlinks
RewriteEngine on

# map neat URL to internal URL
RewriteRule ^member/list/([0-9]+)$   RestController.php?page_key=list&id=$1 [nc,qsa]
RewriteRule ^member/list$   RestController.php?page_key=list [nc,qsa]

RewriteRule ^member/create/$   RestController.php?page_key=create [L]
RewriteRule ^member/create$   mobile/create/ [L,R=301]

RewriteRule ^member/delete/([0-9]+)/$   RestController.php?page_key=delete&id=$1 [L]
RewriteRule ^member/delete([0-9]+)$   mobile/delete/$1 [L,R=301]

RewriteRule ^member/update/([0-9]+)/$   RestController.php?page_key=update&id=$1 [L]
RewriteRule ^member/update/([0-9]+)$   mobile/update/$1/ [L,R=301]